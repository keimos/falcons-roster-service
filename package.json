    "start": "NODE_ENV=local profile=local PORT=8080 node ./dist/server.js --use_strict",
    "test": "NODE_ENV=local profile=test nyc mocha -w --reporter spec --compilers ts:ts-node/register 'src/**/*.test.ts'",
    "test:single": "NODE_ENV=local profile=test nyc --reporter=lcovonly mocha --reporter mocha-junit-reporter --exit --compilers ts:ts-node/register 'src/**/*.test.ts'",
    "coverage": "nyc mocha --compilers ts:ts-node/register 'src/**/*.test.ts'",
    "report": "nyc report",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "start:local": "concurrently --raw \"gulp watch\" \"NODE_ENV=local profile=local timeout=5000 PORT=8080 KafkaTopic=COMOrderRecallPublish-QA KafkaCluster=com-kafka-qa.com.homedepot.com:9092 MONGO_ENCRYPTED=true MONGO_URI=U2FsdGVkX19Q4eW7vNgxYklXBOGSljJd6oh02e3LBWPEWlWG2keS6vd4WBGZQQAhJl2XkBpUX2VT5z4ifcgMTe4vMnQQSFWdHK0EmYM4m2zeNtijPAn94iuGWtT3gqFlzpP5EgBAQvjCZchoMp6wnfKdA6oQ11bya8epK7IEh47rIesJ5Pjj4M3iRcPRGeNsQCJfHWwqQUCNxbg/DjsuW7JnN2ShUlXmlsebd5nQbSuK48TGNV6Mi0lRNFGV07c/ MONGO_DB=qars1db157 TRACKING_SERVICE_API=http://TrackingService.apps.homedepot.com/api/v1/carrierevents nodemon ./dist/server.js --use_strict\"",
    "start:QA": "NODE_ENV=QA profile=QA node ./dist/server.js --use_strict",
    "start:Q2": "NODE_ENV=Q2 profile=Q2 node ./dist/server.js --use_strict",
    "build:QA": "gulp build-qa",
    "build:Q2": "gulp build-q2",
    "build:PR": "gulp build-prod"
  },
  "engines": {},
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.homedepot.com/CustomerDelivery/COMOrderDetailService.git"
  },
